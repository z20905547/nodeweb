block content
	div.topbar
		div.topbar-head
			div.container
				div.row
					div.col-xs-12.col-sm-4.col-md-2.col-lg-2
						span.top_city_item(data-proid='460000') 海南
						span.top_city_item(data-proid='120000') 天津
		div.container
			div.toolbar.row
				div.col-sm-4.logobar.col-md-2.col-lg-2
					img.head-logo(src="/images/logo.png")
				div.small-top-ad.col-xs-0.col-sm-0.col-md-7.col-lg-6
					img.head-static(src="/images/head-static.png")
				div.top-query.col-sm-8.col-md-3.col-lg-4
					div.input-group
						input.form-control(type="text",placeholder="楼盘名称/地产商/位置")
						div.input-group-btn
							button.query-btn.btn.btn-primary(type="submit") 搜索